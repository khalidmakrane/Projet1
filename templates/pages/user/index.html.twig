{% extends 'base.html.twig' %}

{% block title %}Users{% endblock %}

{% block body %}
 <table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
      <tr>
        <td>{{ user.id }}</td>
        <td>{{ user.name }}</td>
        <td>{{ user.email }}</td>
        <td>{{ user.password }}</td>
        <td>{{ user.age }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}
